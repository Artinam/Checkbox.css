@keyframes bouncing-item
    10%, 50%, 80%, 100%
        transform scale(1)

    30%
        transform scale(1.2)

    70%
        transform scale(1.1)

    90%
        transform scale(1.05)

@keyframes check-A
    20%, 50%, 80%, 100%
        transform scale(1) rotate(-45deg)

    30%
        transform scale(1.3) rotate(-45deg)

    70%
        transform scale(1.15) rotate(-45deg)

@keyframes radio-B
    20%, 50%, 80%, 100%
        transform scale(.6)

    40%
        transform scale(.4)

    75%
        transform scale(.55)
